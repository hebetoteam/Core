                                    <div class="d-flex justify-content-between align-items-start flex-wrap mb-2">
                                        <div class="d-flex flex-column mb-5">
                                            <div class="d-flex align-items-center mb-1">
                                                <a href="#" class="text-gray-800 text-hover-warning fs-4 fw-bolder me-3">@Model.Email</a>
                                                @if (Model.EmailConfirmed)
                                                {
                                                    <span class="badge badge-light-success me-auto">Verified</span>
                                                }
                                                else
                                                {
                                                    <span class="badge badge-light-danger me-auto">Unverify</span>
                                                }
                                            </div>
                                        </div>
                                        <form class="w-100 position-relative mb-5" autocomplete="off">
