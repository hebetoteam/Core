@model NetworkViewModel

@{
    ViewBag.Title = "Network";
}

@section Scripts{
    <script src="~/admin-side/demo1/dist/assets/plugins/custom/jstree/jstree.bundle.js"></script>
    <script src="~/admin-app/controllers/network/index.js" asp-append-version="true"></script>

    <script>
        var networkObj = new NetworkController();
        networkObj.initialize();
    </script>
}

@section Styles{
    <link href="~/admin-app/controllers/network/index.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/admin-side/demo1/dist/assets/plugins/custom/jstree/jstree.bundle.css" rel="stylesheet" />
}

<div class="d-flex flex-column flex-column-fluid">

